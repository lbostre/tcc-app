{
  "expo": {
    "name": "tcc-app",
    "owner": "epics-ims",
    "slug": "tcc-app",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "googleServicesFile": "./GoogleService-Info.plist",
      "bundleIdentifier": "com.lthc.tccapp",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/tcc-logo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.lbostre.tccapp",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyAZg61q1ejUnk97tQt6ZJcfkxoTmJ794As"
        }
      },
      "googleServicesFile": "./google-services.json",
      "versionCode": 26
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "@react-native-firebase/app",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/tcc-logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "b7d9a295-0975-4ec5-b61f-7491e500b45a"
      }
    }
  }
}
